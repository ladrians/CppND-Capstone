<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>lsbot_bringup</name>
  <version>0.0.1</version>
  <description>Bringup scripts and configurations for the lsbot stack</description>
  <maintainer email="ladrians@gmail.com">Luciano Silveira</maintainer>
